{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
  },
  "include": [
    "src/**/*.ts",
    "express.d.ts",
    "src/middlewares/**/*.ts",
    "src/models/**/*.ts",
    "src/routes/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}